{
   "baseURL": "https://example.org/",
   "languageCode": "en-us",
   "title": "the cz language",
   "theme": "cz",
   "themesDir": "data/hugo/themes",
   "publishDir": "build/dist/web",
   "relativeURLs": true,
   "module": {
      "mounts": [
         {
            "source": "ideas",
            "target": "content"
         },
         {
            "source": "data/hugo/layouts",
            "target": "layouts"
         },
         {
            "source": "data/hugo/archetypes",
            "target": "archetypes"
         },
         {
            "source": "data/hugo/archetypes",
            "target": "archetypes"
         }
      ]
   },
   "markup": {
      "highlight": {
         "noClasses": false
      }
   }
}